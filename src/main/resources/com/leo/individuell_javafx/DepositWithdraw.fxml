<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="scenePane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.leo.individuell_javafx.controllers.ButtonController">
   <children>
      <TextField fx:id="amountField" layoutX="130.0" layoutY="115.0" prefHeight="85.0" prefWidth="341.0" promptText="Amount">
         <font>
            <Font name="Arial" size="24.0" />
         </font>
      </TextField>
      <Button fx:id="submitAmountButton" layoutX="93.0" layoutY="259.0" mnemonicParsing="false" onAction="#depositAmount" prefHeight="66.0" prefWidth="163.0" text="Submit">
         <font>
            <Font size="24.0" />
         </font>
      </Button>
      <Button fx:id="ReturnButton" layoutX="344.0" layoutY="259.0" mnemonicParsing="false" onAction="#goBack" prefHeight="66.0" prefWidth="163.0" text="Return">
         <font>
            <Font size="24.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
